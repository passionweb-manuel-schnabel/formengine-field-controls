{
  "name": "passionweb/formengine-field-controls",
  "license": "GPL-3.0",
  "description": "Extends the FormEngine and adds a fieldControl button next to the description field. (TYPO3 CMS)",
  "type": "typo3-cms-extension",
  "homepage": "https://www.passionweb.de",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "typo3/cms-core": "^11.5 | ^12.4"
  },
  "autoload": {
    "psr-4": {
      "Passionweb\\FormengineFieldControls\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/formengine-field-controls": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "formengine_field_controls"
    }
  }
}
